<h1>Login</h1>
<form
  id="js-form-login"
>
  <div
    class="form-group"
  >
    <label
      for="js-email"
    >
      Email address
    </label>
    <input
      type="email"
      class="form-control"
      id="js-email"
      aria-describedby="emailHelp"
      placeholder="Email"
      required
    >
  </div>
  <div
    class="form-group"
  >
    <label
      for="js-password"
    >
      Password
    </label>
    <input
      type="password"
      class="form-control"
      id="js-password"
      placeholder="Password"
      required
    >
  </div>
  <button
    type="submit"
    class="btn btn-primary"
  >
    Login
  </button>
</form>
